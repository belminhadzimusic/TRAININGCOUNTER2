<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/diet"
    android:weightSum="1">

    <TextView
        android:text="DAY 1"
        android:textSize="50dp"
        android:layout_marginLeft="130dp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/day11" />

    <TextView
        android:text="Breakfast (266 calories)"
        android:layout_width="match_parent"
        android:textSize="15dp"
        android:layout_height="wrap_content"
        android:id="@+id/day1info" />

    <TextView
        android:text="• 1 slice whole-grain bread"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:textSize="10dp"
        android:id="@+id/textView9" />
    <TextView
        android:text="• 1/4 medium avocado"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:textSize="10dp"
        android:id="@+id/textView10" />
    <TextView
        android:text="• 1 large egg, cooked in 1/4 tsp. olive oil or coat pan with a thin layer of cooking spray (1-second spray)"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:textSize="10dp"
        android:id="@+id/textView11" />
    <TextView
        android:text="• Top egg with a pinch of salt and pepper (1/16 tsp. each)"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:textSize="10dp"
        android:id="@+id/textView12" />
    <TextView
        android:text="• 1 clementine"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:textSize="10dp"
        android:id="@+id/textView13" />

    <TextView
        android:text="Morning Snack (61 calories)"
        android:textSize="15dp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/textView2" />

    <TextView
        android:text="• 1/3 cup blueberries"
        android:textSize="10dp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/textView15" />

    <TextView
        android:text="• 1/4 cup plain non-fat Greek yogurt"
        android:textSize="10dp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/textView14" />

    <TextView
        android:text="Lunch (341 calories)"
        android:textSize="15dp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/textView3" />
    <TextView
        android:text="• 2 cups Ravioli and Vegetable Soup"
        android:textSize="10dp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/textView18" />
    <TextView
        android:text="• 1 Tomato-Cheddar Cheese Toast"
        android:textSize="10dp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/textView133" />
    <TextView
        android:text="Afternoon Snack (93 calories)"
        android:textSize="15dp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/textView555" />
    <TextView
        android:text="• 3 Tbsp. hummus"
        android:textSize="10dp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/textView163" />
    <TextView
        android:text="• 1 cup sliced cucumber"
        android:textSize="10dp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/textView183" />
    <TextView
        android:text="Dinner (451 calories)"
        android:textSize="15dp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/textView7" />

    <TextView
        android:text="Salmon and Vegetables"
        android:textSize="10dp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/textView18y" />
    <TextView
        android:text="• 4 oz. baked salmon"
        android:textSize="10dp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/textView18fy" />
    <TextView
        android:text="• 1 cup roasted Brussels sprouts"
        android:textSize="10dp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/textView18fjuy" />
    <TextView
        android:text="• 1/2 cup brown rice"
        android:textSize="10dp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/textView18fjkuyuy" />
    <TextView
        android:text="• 1 Tbsp. walnuts"
        android:textSize="10dp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/textView18fjkukyuyyuy" />
    <TextView
        android:text="• Salt and pepper to taste (1/8 tsp. each)"
        android:textSize="10dp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/textView18fjkukkyuyyuyyuyyuy" />
    <TextView
        android:text="Vinaigrette"
        android:textSize="10dp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/textView18fjkukkyuyyuyyuylyuyyuy" />
    <TextView
        android:text="• Combine 1 1/2 tsp. each olive oil, lemon juice and maple syrup; season with salt to taste (1/8 tsp.)."
        android:textSize="10dp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/textView18fjkukkyumyyuyyuylyuyyuyyyuyyuylyuyyuy" />
    <TextView
        android:text="Coat Brussels sprouts in 1/2 tsp. olive oil and bake at 425 degrees F until lightly browned, about 15-20 minutes. Coat salmon with 1/4 tsp. olive oil or a thin layer of cooking spray (1 second spray), add salt and pepper to taste (1/8 tsp. each). Bake at 425 degrees F until done, about 4-6 minutes. Serve brown rice, Brussels sprouts and salmon drizzled with vinaigrette and topped with walnuts."
        android:textSize="10dp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/textView18fjkukkyumyyuyyuylyuyyuyyyluyyuylyuyyuyuyyuylyuyyuy" />

</LinearLayout>